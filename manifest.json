{
  "manifest_version": 3,
  "name": "WCA Competitions Tracker",
  "version": "1.2.1",
  "description": "Shows upcoming WCA competitions by country.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }
}