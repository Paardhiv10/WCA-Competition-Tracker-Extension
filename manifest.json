{
  "manifest_version": 3,
  "name": "WCA Competitions Tracker",
  "version": "1.5.3",
  "description": "Shows upcoming WCA competitions by country and finds the closest competitions near you.",
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "permissions": [
    "storage",
    "geolocation"
  ],
  "host_permissions": [
    "https://restcountries.com/*",
    "https://raw.githubusercontent.com/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  }
}